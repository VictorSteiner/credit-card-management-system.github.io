{
  "name": "credit-card-management-system",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:ci": "ng build --prod --sourceMap=false --base-href /credit-card-management-system.github.io/",
    "test": "ng test",
    "format:fix": "prettier --config .prettierrc --write src/**/*.{ts,css,html}",
